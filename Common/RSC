<script>
    function launchGame(gameId) {
        var wrfl = typeof WURFL === 'undefined' || WURFL === null ? WurflFallback() : WURFL;

        var dv = wrfl.is_mobile ? 'm' : 'd';
        document.location.href = document.location.href + '?id=' + gameId + '&d=' + dv;
        return false;
    }

    var AnyGame = 1;

    if (AnyGame > 0) {

        function ActivateTab(tab_index) {
            tab_index = parseInt(tab_index);

            var forall = 1 + 1;
            for (let i = 1; i <= 1; i++) {
                document.querySelector('.tabcontent:nth-child(' + i + ')').style.display = 'none';
            }
            document.querySelector('.tabcontent:nth-child(' + forall + ')').style.display = 'none';
            document.querySelector('.tabcontent:nth-child(' + tab_index + ')').style.display = 'block';

        }
    }
</script>

