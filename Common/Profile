    <script type="text/javascript">
        $(document).ready(function () {
            $("#switchbtndiv").show();
        });

        function ReLogin() {
            location.href = "/Common/Logout";
        }

        function validateForm() {
            isValid = false;
            pwd = $("#NewPassword").val()
            isValid = IsPasswordStrong(pwd);

            if (!isValid) {
                $("#NewPassword").next().html('Kam Sey kam 8 lafaz ka password rekhay jis mey number(123) or alphabets(abc) dono aye.');
                $("#NewPassword").next().addClass('text-danger');
            }
            return isValid;
        }

        function Mainswitch() {
            const btn = document.getElementById("mySwitch");
            if (btn.checked) {
                var Divavailable = document.getElementById('RenderQRModal');
                if (Divavailable == undefined || Divavailable == null) {
                    return;
                }
                $.ajax({
                    type: "GET",
                    url: "/Common/Profile/?handler=ImgOtp",
                    success: function (result) {
                        RevealQr(result);
                    },
                    error: function (exception) {

                    }
                });

            } else {

                $('#resetmodal').modal('show');
                var oldhtml = document.getElementById('qrcodediv');
                var Divavailable = document.getElementById('qrcodediv');
                if (Divavailable != undefined || Divavailable != null) {
                    document.getElementById('qrcodediv').innerHTML = "";
                }
            }
        }
        function RevealQr(result) {
            document.getElementById('RenderQRModal').innerHTML = result;
            $('#QrcodeModal').modal('show');
            var value = document.getElementById('personqrc').value;
            var Qrcode = kjua({ text: value });
            document.querySelector('#qrcodeimagediv').appendChild(Qrcode);
        }
        function VerifyOTP() {
            var secretKey = "";
            var Otp = document.getElementById('otpverify').value;
            if (Otp == "") {
                alert("OTP Code is required");
                return;
            }
            var otpsec = document.getElementById('personsec').value;
            $.ajax({
                type: "GET",
                url: "/Common/Profile/?handler=Verifyotp&secret=" + otpsec + "&otp=" + Otp,
                success: function (result) {
                    if (result.status) {
                        location.reload();
                    } else {
                        alert("Retry! Incorrect/Expired OTP.");
                    }
                },
                error: function (exception) {

                }
            });
        }
        function ResetOtp() {
            var Otp = document.getElementById('resetotpcode').value;
            if (Otp == "") {
                alert("OTP Code is required");
                return;
            }
            var otpsec = '';
            $.ajax({
                type: "GET",
                url: "/Common/Profile/?handler=ResetOtp&secret=" + otpsec + "&otp=" + Otp,
                success: function (result) {
                    if (result.status) {
                        location.reload();
                    } else {
                        alert("Retry with new OTP");
                    }
                },
                error: function (exception) {

                }
            });
        }

    </script>


