<script>
    function ShowExchangeRules() {
        $("#modalMarketRules").modal('show');

        $.get("/rules.html", function (data) {
            $("#modalMarketRules .modal-body").html(data);
        });
    }

    function ShowTos() {
        $("#modalTos").modal('show');
        $.get("/tos.html", function (data) {
            $("#modalTos .modal-body").html(data);
        });
    }
</script>
