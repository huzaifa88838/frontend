        function popup_ChDomain(Url, ClientId) {
            var url = Url + "?ID=" + ClientId;
            newwindow = window.open(url, "Change Domain", 'height=400,width=650,titlebar=0,menubar=0');
            if (window.focus) { newwindow.focus() }
            return false;
        }

        function popup_report(vid) {
            // var url = "/Customer/Statements?VID=" + vid + "&AID=" + aid;
            newwindow = window.open(vid, "Market Repo", 'height=500,width=700,titlebar=0,menubar=0');
            if (window.focus) { newwindow.focus() }
            return false;
        }

        function Getotp(){
            var secode = document.getElementById('otpuserue').value;
            $.ajax({
                type: "GET",
                url: "/Users/Edit/?handler=OtpCode&otp=" + secode,
                success: function (result) {
                    alert("Use this OTP to login = " + result.status);
                },
                error: function (exception) {

                }
            });
        }
