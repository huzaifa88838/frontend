    <script>
        function validateForm() {
            isValid = false;
            pwd = $("#user_Password").val()

            if (pwd === null || pwd === '') {
                return true;
            }

            isValid = IsPasswordStrong(pwd);

            if (!isValid) {
                $("#user_Password").next().text('Min length is 8 characters and password must contain alphabets and numerics.');
                $("#user_Password").next().addClass('text-danger');
            }
            return isValid;
        }

        $("#DId").val(GetDeviceIdentity());
        $(document).ready(function () {
            document.getElementById("iframesection").style.display = 'none';
            document.getElementById("editsection1").style.display = 'block';
            document.getElementById("editsection2").style.display = 'block';


            $('#LedgerBtn').on("click", function (e) {
                e.preventDefault();
                var href = $(this).attr('href');
                $("#iframesection").html('<object data="' + href + '"style="width:100%; height: 90vh;"/>');

                document.getElementById("iframesection").style.display = 'block';
                document.getElementById("editsection1").style.display = 'none';
                document.getElementById("editsection2").style.display = 'none';
            });


            $('#BetsBtn').on("click", function (e) {
                e.preventDefault();
                var href = $(this).attr('href');
                $("#iframesection").html('<object data="' + href + '"style="width:100%; height: 90vh;"/>');

                document.getElementById("iframesection").style.display = 'block';
                document.getElementById("editsection1").style.display = 'none';
                document.getElementById("editsection2").style.display = 'none';
            });


            $('#ProfitlossBtn').on("click", function (e) {
                e.preventDefault();
                var href = $(this).attr('href');
                $("#iframesection").html('<object data="' + href + '"style="width:100%; height: 90vh;"/>');

                document.getElementById("iframesection").style.display = 'block';
                document.getElementById("editsection1").style.display = 'none';
                document.getElementById("editsection2").style.display = 'none';
            });

